{
    "manifest_version": 3,
    "name": "QbitFix",
    "version": "1.0",
    "description": "Додає номери задач куди дотягуються мої хтиві пальчики \n by damn ℜ𝔢𝔪𝔢𝔫𝔬𝔡",
    "permissions": [
        "activeTab"
    ],
    "host_permissions": [
        "https://qbit.dots.org.ua/standings",
        "https://qbit.dots.org.ua/problems",
        "https://qbit.dots.org.ua/solutions"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://qbit.dots.org.ua/standings*",
                "https://qbit.dots.org.ua/problems*",
                "https://qbit.dots.org.ua/solutions*"
            ],
            "js": [ "content.js" ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "incognito": "spanning"
}
